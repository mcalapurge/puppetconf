	// Bootstrap Snippets

	"Bootstrap Cont, Row, Col": {
        "prefix": "controwcol",
        "body": [
            "<div class=\"container\">",
            "  <div class=\"row\">",
            "    <div class=\"col\">",
            "      ",
            "    </div>",
            "  </div>",
            "</div>"
        ],
        "description": "Bootstrap Cont, Row, Col"
	},

	//Table 2x2

	"Table 2x2": {
        "prefix": "tab2x2",
        "body": [
            "<table>",
            "  <tr><th>**</th><th>**</th></tr>",
            "  <tr><td>**</td><td>**</td></tr>",
            "</table>"
        ],
        "description": "Table 2x2"
	},

	"Table 3x2": {
        "prefix": "tab3x2",
        "body": [
            "<table>",
            "  <tr><th>**</th><th>**</th><th>**</th></tr>",
            "  <tr><td>**</td><td>**</td><td>**</td></tr>",
            "</table>"
        ],
        "description": "Table 3x2"
	} 
}