	// Bootstrap Snippets

	"Bootstrap Cont, Row, Col": {
	"prefix": "controwcol",
	"body": [
		"<div class=\"container\">",
		"  <div class=\"row\">",
		"    <div class=\"col\">",
		"      ",
		"    </div>",
		"  </div>",
		"</div>"
	],
	"description": "Bootstrap Cont, Row, Col"
	},

	//Table 2x2

